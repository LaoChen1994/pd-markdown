{
  "extends": "../../tsconfig.json",
  "include": ["src"],
  "compilerOptions": {
    "target": "ESNext",
    "declaration": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite/client", "node", "@pdchen/markdown-typings", "./typings.d.ts"]
  }
}
